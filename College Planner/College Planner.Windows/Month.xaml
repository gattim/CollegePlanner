        <local:Day x:Name="day13" PointerPressed="Day_PointerPressed" Grid.Row="1" Grid.Column="5"/>
        <local:Day x:Name="day14" PointerPressed="Day_PointerPressed" Grid.Row="1" Grid.Column="6"/>
        <local:Day x:Name="day15" PointerPressed="Day_PointerPressed" Grid.Row="2" Grid.Column="0"/>
        <local:Day x:Name="day16" PointerPressed="Day_PointerPressed" Grid.Row="2" Grid.Column="1"/>
